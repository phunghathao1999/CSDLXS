            {
                string S = this.queryString;
                //Kiểm tra câu truy vấn có hợp lệ
                if (!this.CheckStringQuery(S))
                {
                    return false;
                }


                //Get All Relation
                this.selectedRelations = GetAllRelation(S);
                

                if (this.selectedRelations == null)
                {
                    return false;
                }
                //format AttributeName
